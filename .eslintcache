t-services.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Loading/Loading.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/user-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/message-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Friends/Friends.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/XatFriends/XatFriends.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/NewDoubt/NewDoubt.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/SearchFriend/SearchFriend.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Footer/Footer.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PerfilPublico/PerfilPublico.js",["155","156"],"import React, { Component } from 'react'\nimport UserService from '../../services/user-service'\nimport './PerfilPublico.css'\n\n\nclass perfilPublico extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            userProfile: '',\n            loggedInUser: ''\n        }\n        this.userService = new UserService()\n    }\n\n    componentDidMount = () => {\n        this.userService\n            .getUser(this.props.publicProfileId)\n            .then(response => {\n                this.setState({\n                    userProfile: response,\n                    loggedInUser: this.props.loggedInUser\n                })\n                this.isFriendToggle(this.props.loggedInUser.friends, response._id)\n            })\n    }\n\n    renderProfile = () => {\n        return (\n            <div>\n                <img src={this.state.userProfile.imgPath} alt={this.state.userProfile.imgName} />\n                <h1>{this.state.userProfile.name} {this.state.userProfile.lastName}</h1>\n                <p><span>Email: </span> {this.state.userProfile.email}</p>\n                <p><span>Ciudad: </span> {this.state.userProfile.city}</p>\n                <p><span>Curso de Ironhack: </span>{this.state.userProfile.ironhackCourse}</p>\n                <p><span>GitHub: </span>{this.state.userProfile.githubUrl && <a href={this.state.userProfile.githubUrl} target='_blank'>Ver perfil</a>}</p>\n                <p><span>Linkedin: </span>{this.state.userProfile.linkedinUrl && <a href={this.state.userProfile.linkedinUrl} target='_blank'>Ver perfil</a>}</p>\n                {\n                    this.state.isFriend\n                    ? this.renderButtonDeleteFriend()\n                    : this.renderButtonFriendship()\n                }\n            </div>\n        )\n    }\n    isFriendToggle = (arr, profileId) => {\n        const isFriend = arr.filter(friend => {\n            return friend._id === profileId\n        })\n        if(isFriend.length > 0){\n            this.setState({isFriend: true})\n            return\n        }else {\n            this.setState({isFriend: false})\n            return\n        }\n    }\n    renderButtonFriendship = () => {\n        \n            return(\n                <button onClick={(e) => this.addFriend(e)}>Añadir amistad</button>\n            )\n\n    }\n    renderButtonDeleteFriend = () => {\n            return(\n                <button onClick={(e) => this.deleteFriend(e)}>Eliminar amistad</button>\n            )\n        \n    }\n\n    addFriend = (e) => {\n        e.preventDefault()\n        this.userService\n            .addFriend(this.props.loggedInUser._id, this.state.userProfile._id)\n            .then(response => {\n                this.props.updateFriendsAdd(this.state.userProfile)\n                this.setState({isFriend: true})\n            })\n    }\n    deleteFriend = (e) => {\n        e.preventDefault()\n        this.userService\n            .deleteFriend(this.props.loggedInUser._id, this.state.userProfile._id)\n            .then(response => {\n                this.props.updateFriendsDelete(this.state.userProfile)\n                this.setState({isFriend: false})\n            })\n\n    }\n\n\n    render() {\n        return(\n            <div className='perfil-publico'>\n                {this.state.userProfile && this.renderProfile()}\n            </div>\n        )\n\n    }\n}\n\nexport default perfilPublico","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Info/Info.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/EditProfileForm/EditProfileForm.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/SearchDoubt/SearchDoubt.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/EditDoubtForm/EditDoubtForm.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/NewAnswer/NewAnswer.js",[],["157","158"],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/auth/ProtectedRoute.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Verificating/Verificating.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/FormResendEmail/FormResendEmail.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/FormResetPwd/FormResetPwd.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/FormNewPwd/FormNewPwd.js",[],{"ruleId":"159","replacedBy":"160"},{"ruleId":"161","replacedBy":"162"},{"ruleId":"163","severity":1,"message":"164","line":36,"column":121,"nodeType":"165","endLine":36,"endColumn":136},{"ruleId":"163","severity":1,"message":"164","line":37,"column":127,"nodeType":"165","