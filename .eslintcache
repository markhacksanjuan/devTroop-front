n\n    }\n\n    componentWillReceiveProps = (nextProps) => {\n        this.setState({...this.state, loggedInUser: nextProps['userInSession']})\n    }\n\n    logoutUser = () => {\n        this.service.logout()\n        .then(() => {\n            this.setState({ loggedInUser: null })\n            this.props.getUser(null)\n        })\n    }\n\n    render() {\n\n        if(this.state.loggedInUser){\n            return(\n                <div className='navbar'>\n                        <Link to='/profile'>\n                        <img id=\"img-user\" src={this.props.userInSession.imgPath} />\n                        </Link>\n                        <Link to='/pool'>Dudas</Link>\n                        <Link to='/message'>IronXat</Link>\n\n                </div>\n            )\n\n        }else {\n            return(\n                <div className='navbar'>\n                        <Link to='/'><img id='img-devtroop' src={devTroop} /></Link>\n                        <Link to='/pool'>Dudas</Link>\n                </div>\n            )\n        }\n\n    }\n}\n\nexport default Navbar","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Home/Home.js",["158"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport './Home.css'\n\nimport devTroop from '../../img/devtroop_solidstroke.gif'\n\nconst home = () => {\n    return(\n        <div className='home'>\n            <img src={devTroop} />\n            <h1>Bienvenido a devTroop</h1>\n            <h4>La tropa de desarrolladores de Ironhack ha llegado para quedarse!</h4>\n            <div className='home-links'>\n                <Link to='/signup'>Regístrate</Link>\n                <Link to='/login'>Log In</Link>\n            </div>\n        </div>\n    )\n}\n\nexport default home","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Signup/Signup.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Login/Login.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Verification/Verification.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/PoolDoubts.js",["159","160","161","162"],"import React, { Component } from 'react'\nimport { Link, Switch, Route } from 'react-router-dom'\nimport './PoolDoubts.css'\n\n// --- SERVICES ---\nimport DoubtService from '../../services/doubt-services'\n\n// --- COMPONENTS ---\nimport DoubtList from './DoubtList/DoubtList'\nimport OneDoubt from './OneDoubt/OneDoubt'\nimport Answers from './Answers/Answers'\nimport NewDoubt from './NewDoubt/NewDoubt'\nimport Loading from '../Loading/Loading'\nimport SearchDoubt from './SearchDoubt/SearchDoubt'\nimport NewAnswer from './NewAnswer/NewAnswer'\nimport EditDoubtForm from '../EditDoubtForm/EditDoubtForm'\n\nclass PoolDoubts extends Component {\n\n    constructor(props){\n        super(props)\n        this.state = {\n            doubts: [],\n            selectedDoubt: '',\n            doubtUser: '',\n            doubtAnswers: '',\n            showForm: false,\n            showFormAnswer: false,\n            doubtsSearch: []\n\n        }\n        this.service = new DoubtService()\n    }\n\n    componentDidMount = () => {\n        this.service\n        .getAllDoubts()\n        .then(response => {\n            this.setState({\n                doubts: response,\n                doubtsSearch: response,\n            })\n        })\n    }\n\n    getDoubt = (id) => {\n        const selectedDoubt = this.state.doubts.filter(doubt => {\n            return doubt._id === id\n        })\n        this.service.getAllAnswersOfDoubt(selectedDoubt[0]._id)\n        .then(answers => {\n            this.setState({\n                selectedDoubt,\n                doubtAnswers: answers\n            })\n            \n        })\n        this.props.getDoubtToApp(selectedDoubt)\n    }\n\n    showFormToggle = (e) => {\n        e.preventDefault()\n        this.state.showForm ? this.setState({showForm: false}) : this.setState({showForm: true})\n    }\n    showFormAnswerToggle = (e) => {\n        e.preventDefault()\n        this.state.showFormAnswer ? this.setState({showFormAnswer: false}) : this.setState({showFormAnswer: true})\n    }\n\n    search = (value) => {\n        const copyDoubts = [...this.state.doubtsSearch]\n        const valueLower = value.toLowerCase()\n        const newArr = copyDoubts.filter(item => {\n            const newName = item.title.toLowerCase()\n            return newName.includes(valueLower)\n        })\n        this.setState({\n            doubts: newArr\n        })\n        if(value === ''){\n            this.setState({ doubts: copyDoubts })\n        }\n    }\n    \n\n\n\n    render = () => {\n        return(\n            <div className='pool-doubts'>\n                <h1>¡Hey! ¿Tienes alguna duda?</h1>\n                {this.props.loggedInUser && \n                <button className='pool-button' onClick={(e) => this.showFormToggle(e)}>Crea tu duda</button>\n                }\n                    <NewDoubt\n                        show={this.state.showForm}\n                        loggedInUser={this.props.loggedInUser}\n                    />\n                    \n                    <div className='container-doubt-answer'>\n                        <div className='doubts'>\n                        <SearchDoubt\n                            search={this.search}\n                            />\n                        {this.state.doubts.length === 0 ? <Loading /> : <DoubtList \n                                                getDoubt={this.getDoubt}\n                                                doubts={this.state.doubts} />}\n                        </div>\n                        <div className='doubt-answers'>\n                        {this.state.selectedDoubt === '' ? null : <OneDoubt \n                                selectedDoubt={this.state.selectedDoubt[0]}\n                                loggedInUser={this.props.loggedInUser}\n                            />}\n                        {this.state.doubtAnswers === '' ? null : <Answers \n                                doubtAnswers={this.state.doubtAnswers}\n                                loggedInUser={this.props.loggedInUser}\n                                getDoubt={this.getDoubt}\n                                selectedDoubt={this.state.selectedDoubt[0]}\n                            />}\n                <NewAnswer \n                    show={this.state.showFormAnswer}\n                    loggedInUser={this.props.loggedInUser}\n                    selectedDoubt={this.state.selectedDoubt[0]}\n                    getDoubt={this.getDoubt}\n                    showFormAnswerToggle={this.showFormAnswerToggle}\n                />\n                            {(this.props.loggedInUser && this.state.doubtAnswers !== '') &&\n                <button className='pool-button' onClick={(e) => this.showFormAnswerToggle(e)}>Responde a la duda</button>\n                }\n                        </div>\n\n\n                    </div>\n\n                    \n            </div>\n        )\n    }\n\n}\n\nexport default PoolDoubts","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/DoubtList/DoubtList.js",["163","164","165"],"import React, { Component } from 'react'\nimport { Link, Route } from 'react-router-dom'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport './DoubtList.css'\n\nconst DoubtList = (props) => {\n\n    const handleClick = (e, id) => {\n        e.preventDefault()\n        props.getDoubt(id)\n    }\n\n    const renderList = () => {\n        return props.doubts.map((doubt, index) => {\n            return (\n                <li key={index}>\n                    <button  onClick={(e) => handleClick(e, doubt._id)} >\n                        {doubt.title}\n                    </button>\n                </li>\n            )\n        })\n    }\n\n    return(\n        <div className='doubt-list'>\n        \n            <Scrollbars \n            style={{ width: 300, heigth: 500 }}>\n                <ul>\n                    {renderList()}\n                </ul>\n\n            </Scrollbars>\n        </div>\n    )\n\n}\n\nexport default DoubtList","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/OneDoubt/OneDoubt.js",["166"],"import React, { Component } from 'react'\nimport { Link } from 'react-router-dom'\nimport './OneDoubt.css'\n\nconst OneDoubt = (props) => {\n\n    const renderEditButton = () => {\n        if(props.loggedInUser){\n            return(\n                props.selectedDoubt.userId.email !== props.loggedInUser.email \n                    ? null \n                    : <Link to='/editDoubt'>Editar duda</Link>\n            )\n        } else { return null }\n    }\n\n    return(\n        <div className='one-doubt'>\n            <h3>{props.selectedDoubt.title}</h3>\n            <p>{props.selectedDoubt.doubt}</p>\n            <p>Escrita por: {props.selectedDoubt.userId.name} {props.selectedDoubt.userId.lastName}</p>\n\n            { \n            renderEditButton()\n            }\n        </div>\n    )\n\n}\n\nexport default OneDoubt","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/Answers/Answers.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/Message.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/Xat/Xat.js",["167"],"import React, { Component } from 'react'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport './Xat.css'\n\n// --- SERVICES ---\nimport MessageService from '../../../services/message-service'\n\n// --- COMPONENTS ---\nimport Loading from '../../Loading/Loading'\n\nclass Xat extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            newMessage: '',\n            xat: [],\n            toUser: '',\n        }\n        this.MessageService = new MessageService()\n        this.scrollbars = React.createRef()\n    }\n\n    componentDidMount = () => {\n        this.setState({\n            toUser: this.props.toUser,\n            xat: this.props.xat,\n        })\n    }\n\n    componentDidUpdate = () => {\n        this.scrollbars.current.scrollToBottom()\n    }\n\n    handleFormSubmit = (event) => {\n        event.preventDefault()\n        const { newMessage } = this.state\n        this.props.createNewMessage(newMessage)\n        this.setState({\n            newMessage: ''\n        })\n    }\n\n    handleChange = (event) => {\n        const { name, value } = event.target\n        this.setState({ [name]: value })\n    }\n\n\n    renderXat = () => {\n        const xat = [...this.props.xat]\n            return xat.map((message,index) => {\n                return <li key={index}><img src={message.fromUserId.imgPath} /> - {message.message}</li>\n        })\n        \n    }\n\n    render() {\n        return (\n            <div className='xat'>\n                <div className='messages'>\n                    <Scrollbars \n                    ref={this.scrollbars}\n                    style={{ width: 300, height: 250 }}>\n    \n                        <ul>\n                    {this.props.toUser === '' ? <Loading /> : this.renderXat()}\n                        </ul>\n    \n                    </Scrollbars>\n                </div>\n                <form onSubmit={this.handleFormSubmit}>\n                    <input className='input-xat' name='newMessage' value={this.state.newMessage} onChange={ e => this.handleChange(e)} autoComplete='off' placeholder='Escribe tu mensaje' autoFocus />\n                    <button type='submit'>Enviar</button>\n    \n                </form>\n                \n    \n            </div>\n        )\n    }\n}\n\nexport default Xat","/home/mark/Ir