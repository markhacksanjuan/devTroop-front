 super(props)\n        this.state = {\n            loggedInUser: null\n        }\n        this.service = new AuthService()\n\n    }\n\n    componentWillReceiveProps = (nextProps) => {\n        this.setState({...this.state, loggedInUser: nextProps['userInSession']})\n    }\n\n    logoutUser = () => {\n        this.service.logout()\n        .then(() => {\n            this.setState({ loggedInUser: null })\n            this.props.getUser(null)\n        })\n    }\n\n    render() {\n\n        if(this.state.loggedInUser){\n            return(\n                <div className='navbar'>\n                    <div className='nav-home'>\n                        <Link to='/'><img src={devTroop} /></Link>\n                        <Link to='/profile'>Perfil</Link>\n                        <Link to='/pool'>Dudas</Link>\n                        <Link to='/message'>IronXat</Link>\n                    </div>\n                    <div>\n                        <p>Hola, {this.state.loggedInUser.name}</p>\n                    </div>\n                    <div>\n                        <Link to='/'>\n                        <button onClick={() => this.logoutUser()}>Log Out</button>\n                        </Link>\n                    </div>\n                </div>\n            )\n\n        }else {\n            return(\n                <div className='navbar'>\n                    <div className='nav-home'>\n                        <Link to='/'><img src={devTroop} /></Link>\n                        <Link to='/pool'>Dudas</Link>\n                    </div>\n                    <div>\n                    <Link to='/signup'>Sign Up</Link>\n                        <Link to='/login'>Log In</Link>\n                    </div>\n                </div>\n            )\n        }\n\n    }\n}\n\nexport default Navbar","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Home/Home.js",["107","108"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimpo