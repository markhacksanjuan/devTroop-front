essage extends Component {\n\n    constructor(props){\n        super(props)\n        this.state = {\n            friends: [],\n            toUser: '',\n            xat: [],\n            newMessage: '',\n            allMessages: ''\n        }\n        this.service = new UserService()\n        this.messageService = new MessageService()\n    }\n\n    componentDidMount = () => {\n        this.service\n        .getFriends(this.props.loggedInUser._id)\n        .then(response => {\n            this.messageService.getAll(this.props.loggedInUser._id)\n            .then(result => {\n                this.setState({\n                    friends: response,\n                    allMessages: result\n                })\n            })\n        })\n    }\n    getXat = (id) => {\n        this.messageService\n            .getAll(id)\n            .then(result => {\n                // this.getXatById(this.state.toUser)\n            })\n    }\n    getXatById = (id) => {\n        const toUser = this.state.friends.filter(item => {\n            return item._id === id\n        })\n        this.setState({toUser: toUser[0]})\n        const copyAllMessages = [...this.state.allMessages]\n        const messagesArr = copyAllMessages.filter(message => {\n            if(!message.fromUserId || !message.toUserId){\n                return \n            }else {\n                return message.toUserId === toUser[0]._id || message.fromUserId._id === toUser[0]._id\n            }\n        })\n        console.log(toUser[0]._id)\n        this.setState({\n            xat: messagesArr,\n            toUser: toUser[0]\n        })        \n    }\n    createNewMessage = (newMessage) => {\n        this.messageService\n        .sendMessage(newMessage, this.state.toUser._id, this.props.loggedInUser._id)\n        .then(() => {\n            this.messageService.getAll(this.props.loggedInUser._id)\n            .then(response => {\n                this.setState({\n                    allMessages: response\n                })\n                this.getXatById(this.state.toUser._id)\n            })\n        })\n        .catch(err => console.error(err))\n    }\n    updateXat = () => {\n        setInterval(() => {\n            this.messageService.getAll(this.props.loggedInUser._id)\n            .then(result => {\n                this.setState({\n                    allMessages: result\n                })\n            })\n        }, 1000);\n    }\n\n\n\n    render() {\n        return (\n            <div className='message-container'>\n                <h1>IronXat</h1>\n                <div className='message'>\n                    <div>\n                        {this.state.friends.length === 0 ? <Loading /> : <XatFriends \n                            friends={this.state.friends}\n                            getXatById={this.getXatById}\n                        />}\n                    </div>\n                    <div>\n                            {this.state.toUserId === '' ? <Loading /> : <Xat \n                            toUser={this.state.toUser}\n                            loggedInUser={this.props.loggedInUser}\n                            createNewMessage={this.createNewMessage}\n                            xat={this.state.xat}\n                            getXat={this.getXat}\n                            getXatById={this.getXatById}\n                            allMessages={this.state.allMessages}\n                        />}\n                        \n                    </div>\n                </div>\n    \n    \n            </div>\n        )\n    }\n}\n\nexport default Message","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/Xat/Xat.js",["157"],"import React, { Component } from 'react'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport './Xat.css'\n\n// --- SERVICES ---\nimport MessageService from '../../../services/message-service'\n\n// --- COMPONENTS ---\nimport Loading from '../../Loading/Loading'\n\nclass Xat extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            newMessage: '',\n            xat: [],\n            toUser: '',\n        }\n        this.messageService = new MessageService()\n        this.scrollbars = React.createRef()\n    }\n\n    componentDidMount = () => {\n        this.setState({\n            toUser: this.props.toUser,\n            xat: this.props.xat,\n            allMessages: this.props.allMessages\n        })\n        this.timer = setInterval(this.updateXat, 4000)\n    }\n\n    updateXat = () => {\n        if(this.props.toUser){\n            this.messageService\n                .getAll(this.props.loggedInUser._id)\n                .then((messages) => {\n                    const messagesArr = messages.filter(message => {\n                        if(!message.toUserId || !message.fromUserId){\n                            console.log(message.toUserId)\n                            return \n                        }else {\n                            return message.toUserId === this.props.toUser._id || message.fromUserId._id === this.props.toUser._id\n                        }\n                    })\n                    this.setState({\n                        xat: messagesArr\n                    })\n                })\n        }\n    }\n\n    componentDidUpdate = () => {\n        this.scrollbars.current.scrollToBottom()\n    }\n    componentWillUnmount = () => {\n        clearInterval(this.timer)\n    }\n\n    handleFormSubmit = (event) => {\n        event.preventDefault()\n        const { newMessage } = this.state\n        this.props.createNewMessage(newMessage)\n        this.setState({\n            newMessage: ''\n        })\n    }\n\n    handleChange = (event) => {\n        const { name, value } = event.target\n        this.setState({ [name]: value })\n    }\n\n\n    renderXat = () => {\n        const xat = [...this.state.xat]\n            return xat.map((message,index) => {\n                return <li key={index}><img src={message.fromUserId.imgPath} alt={message.fromUserId.imgName} /> - {message.message}</li>\n        })\n        \n    }\n\n    render() {\n        return (\n            <div className='xat'>\n                <div className='messages'>\n                    <Scrollbars \n                    ref={this.scrollbars}\n                    style={{ width: 300, height: 250 }}>\n    \n                        <ul>\n                    {this.props.toUser === '' ? <Loading /> : this.renderXat()}\n                        </ul>\n    \n                    </Scrollbars>\n                </div>\n                <form onSubmit={this.handleFormSubmit}>\n                    <input className='input-xat' name='newMessage' value={this.state.newMessage} onChange={ e => this.handleChange(e)} autoComplete='off' placeholder='Escribe tu mensaje' />\n                    <button type='submit'>Enviar</button>\n    \n                </form>\n                \n    \n            </div>\n        )\n    }\n}\n\nexport default Xat","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Perfil.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Doubts/Doubts.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/doubt-services.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Loading/Loading.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/user-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/message-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Friends/Friends.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/XatFriends/XatFriends.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/NewDoubt/NewDoubt.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/SearchFriend/SearchFriend.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Footer/Footer.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PerfilPublico/PerfilPublico.js",["158","159"],"import React, { Component } from 'react'\nimport UserService from '../../services/user-service'\nimport './PerfilPublico.css'\n\n\nclass perfilPublico extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            userProfile: '',\n            loggedInUser: ''\n        }\n        this.userService = new UserService()\n    }\n\n    componentDidMount = () => {\n        this.userService\n            .getUser(this.props.publicProfileId)\n            .then(response => {\n                this.setState({\n                    userProfile: response,\n                    loggedInUser: this.props.loggedInUser\n                })\n                this.isFriendToggle(this.props.loggedInUser.friends, response._id)\n            })\n    }\n\n    renderProfile = () => {\n        return (\n            <div>\n                <img src={this.state.userProfile.imgPath} alt={this.state.userProfile.imgName} />\n                <h1>{this.state.userProfile.name} {this.state.userProfile.lastName}</h1>\n                <p><span>Email: </span> {this.state.userProfile.email}</p>\n                <p><span>Ciudad: </span> {this.state.userProfile.city}</p>\n                <p><span>Curso de Ironhack: </span>{this.state.userProfile.ironhackCourse}</p>\n                <p><span>GitHub: </span>{this.state.userProfile.githubUrl && <a href={this.state.userProfile.githubUrl} target='_blank'>Ver perfil</a>}</p>\n                <p><span>Linkedin: </span>{this.state.userProfile.linkedinUrl && <a href={this.state.userProfile.linkedinUrl} target='_blank'>Ver perfil</a>}</p>\n                {\n                    this.state.isFriend\n                    ? this.renderButtonDeleteFriend()\n                    : this.renderButtonFriendship()\n                }\n            </div>\n        )\n    }\n    isFriendToggle = (arr, profileId) => {\n        const isFriend = arr.filter(friend => {\n            return friend._id === profileId\n        })\n        if(isFriend.length > 0){\n            this.setState({isFriend: true})\n            return\n        }else {\n            this.setState({isFriend: false})\n            return\n        }\n    }\n    renderButtonFriendship = () => {\n        \n            return(\n                <button onClick={(e) => this.addFriend(e)}>Añadir amistad</button>\n            )\n\n    }\n    renderButtonDeleteFriend = () => {\n            return(\n                <button onClick={(e) => this.deleteFriend(e)}>Eliminar amistad</button>\n            )\n        \n    }\n\n    addFriend = (e) => {\n        e.preventDefault()\n        this.userService\n            .addFriend(this.props.loggedInUser._id, this.state.userProfile._id)\n            .then(response => {\n                this.props.updateFriendsAdd(this.state.userProfile)\n                this.setState({isFriend: true})\n            })\n    }\n    deleteFriend = (e) => {\n        e.preventDefault()\n        this.userService\n            .deleteFriend(this.props.loggedInUser._id, this.state.userProfile._id)\n            .then(response => {\n                this.props.updateFriendsDelete(this.state.userProfile)\n                this.setState({isFriend: false})\n            })\n\n    }\n\n\n    render() {\n        return(\n            <div className='perfil-publico'>\n                {this.state.userProfile && this.renderProfile()}\n            </div>\n        )\n\n    }\n}\n\nexport default perfilPublico","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Info/Info.js",[],"/home/mark/Ironhack/bootcamp/semana8/f