messages":"166","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/index.js",[],["167","168"],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/App.js",["169"],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/reportWebVitals.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/auth/auth-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Navbar/Navbar.js",["170","171"],"import React, { Component } from 'react'\nimport { Link } from 'react-router-dom'\nimport './Navbar.css'\nimport AuthService from '../../auth/auth-service'\n\nimport devTroop from '../../img/devtroopbucle.gif'\n\nclass Navbar extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            loggedInUser: null\n        }\n        this.service = new AuthService()\n\n    }\n\n    componentWillReceiveProps = (nextProps) => {\n        this.setState({...this.state, loggedInUser: nextProps['userInSession']})\n    }\n\n    logoutUser = () => {\n        this.service.logout()\n        .then(() => {\n            this.setState({ loggedInUser: null })\n            this.props.getUser(null)\n        })\n    }\n\n    render() {\n\n        if(this.state.loggedInUser){\n            return(\n                <div className='navbar'>\n                        <Link to='/profile'>\n                        <img id=\"img-user\" src={this.props.userInSession.imgPath} />\n                        </Link>\n                        <Link to='/pool'>Dudas</Link>\n                        <Link to='/message'>IronXat</Link>\n\n                </div>\n            )\n\n        }else {\n            return(\n                <div className='navbar'>\n                        <Link to='/'><img id='img-devtroop' src={devTroop} /></Link>\n                        <Link to='/pool'>Dudas</Link>\n                </div>\n            )\n        }\n\n    }\n}\n\nexport default Navbar","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Home/Home.js",["172"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport './Home.css'\n\nimport devTroop from '../../img/devtroop_solidstroke.gif'\n\nconst home = () => {\n    return(\n        <div className='home'>\n            <img src={devTroop} />\n            <h1>Bienvenido a devTroop</h1>\n            <h4>La tropa de desarrolladores de Ironhack ha llegado para quedarse!</h4>\n            <div className='home-links'>\n                <Link to='/signup'>Regístrate</Link>\n                <Link to='/login'>Log In</Link>\n            </div>\n        </div>\n    )\n}\n\nexport default home","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Signup/Signup.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Login/Login.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Verification/Verification.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/PoolDoubts.js",["173","174","175","176"],"import React, { Component } from 'react'\nimport { Link, Switch, Route } from 'react-router-dom'\nimport './PoolDoubts.css'\n\n// --- SERVICES ---\nimport DoubtService from '../../services/doubt-services'\n\n// --- COMPONENTS ---\nimport DoubtList from './DoubtList/DoubtList'\nimport OneDoubt from './OneDoubt/OneDoubt'\nimport Answers from './Answers/Answers'\nimport NewDoubt from './NewDoubt/NewDoubt'\nimport Loading from '../Loading/Loading'\nimport SearchDoubt from './SearchDoubt/SearchDoubt'\nimport NewAnswer from './NewAnswer/NewAnswer'\nimport EditDoubtForm from '../EditDoubtForm/EditDoubtForm'\n\nclass PoolDoubts extends Component {\n\n    constructor(props){\n        super(props)\n        this.state = {\n            doubts: [],\n            selectedDoubt: '',\n            doubtUser: '',\n            doubtAnswers: '',\n            showForm: false,\n            showFormAnswer: false,\n            doubtsSearch: []\n\n        }\n        this.service = new DoubtService()\n    }\n\n    componentDidMount = () => {\n        this.service\n        .getAllDoubts()\n        .then(response => {\n            this.setState({\n                doubts: response,\n                doubtsSearch: response,\n            })\n        })\n    }\n\n    getDoubt = (id) => {\n        const selectedDoubt = this.state.doubts.filter(doubt => {\n            return doubt._id === id\n        })\n        this.service.getAllAnswersOfDoubt(selectedDoubt[0]._id)\n        .then(answers => {\n            this.setState({\n                selectedDoubt,\n                doubtAnswers: answers\n            })\n            \n        })\n        this.props.getDoubtToApp(selectedDoubt)\n    }\n\n    showFormToggle = (e) => {\n        e.preventDefault()\n        this.state.showForm ? this.setState({showForm: false}) : this.setState({showForm: true})\n    }\n    showFormAnswerToggle = (e) => {\n        e.preventDefault()\n        this.state.showFormAnswer ? this.setState({showFormAnswer: false}) : this.setState({showFormAnswer: true})\n    }\n\n    search = (value) => {\n        const copyDoubts = [...this.state.doubtsSearch]\n        const valueLower = value.toLowerCase()\n        const newArr = copyDoubts.filter(item => {\n            const newName = item.title.toLowerCase()\n            return newName.includes(valueLower)\n        })\n        this.setState({\n            doubts: newArr\n        })\n        if(value === ''){\n            this.setState({ doubts: copyDoubts })\n        }\n    }\n    \n\n\n\n    render = () => {\n        return(\n            <div className='pool-doubts'>\n                <h1>¡Hey! ¿Tienes alguna duda?</h1>\n                {this.props.loggedInUser && \n                <button className='pool-button' onClick={(e) => this.showFormToggle(e)}>Crea tu duda</button>\n                }\n                    <NewDoubt\n                        show={this.state.showForm}\n                        loggedInUser={this.props.loggedInUser}\n                    />\n                    \n                    <div className='container-doubt-answer'>\n                        <div className='doubts'>\n                        <SearchDoubt\n                            search={this.search}\n                            />\n                        {this.state.doubts.length === 0 ? <Loading /> : <DoubtList \n                                                getDoubt={this.getDoubt}\n                                                doubts={this.state.doubts} />}\n                        </div>\n                        <div className='doubt-answers'>\n                        {this.state.selectedDoubt === '' ? null : <OneDoubt \n                                selectedDoubt={this.state.selectedDoubt[0]}\n                                loggedInUser={this.props.loggedInUser}\n                            />}\n                        {this.state.doubtAnswers === '' ? null : <Answers \n                                doubtAnswers={this.state.doubtAnswers}\n                                loggedInUser={this.props.loggedInUser}\n                                getDoubt={this.getDoubt}\n                                selectedDoubt={this.state.selectedDoubt[0]}\n                            />}\n                <NewAnswer \n                    show={this.state.showFormAnswer}\n                    loggedInUser={this.props.loggedInUser}\n                    selectedDoubt={this.state.selectedDoubt[0]}\n                    getDoubt={this.getDoubt}\n                    showFormAnswerToggle={this.showFormAnswerToggle}\n                />\n                            {(this.props.loggedInUser && this.state.doubtAnswers !== '') &&\n                <button className='pool-button' onClick={(e) => this.showFormAnswerToggle(e)}>Responde a la duda</button>\n                }\n                        </div>\n\n\n                    </div>\n\n                    \n            </div>\n        )\n    }\n\n}\n\nexport default PoolDoubts","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/DoubtList/DoubtList.js",["177","178","179"],"import React, { Component } from 'react'\nimport { Link, Route } from 'react-router-dom'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport './DoubtList.css'\n\nconst DoubtList = (props) => {\n\n    const handleClick = (e, id) => {\n        e.preventDefault()\n        props.getDoubt(id)\n    }\n\n    const renderList = () => {\n        return props.doubts.map((doubt, index) => {\n            return (\n                <li key={index}>\n                    <button  onClick={(e) => handleClick(e, doubt._id)} >\n                        {doubt.title}\n                    </button>\n                </li>\n            )\n        })\n    }\n\n    return(\n        <div className='doubt-list'>\n        \n            <Scrollbars \n            style={{ width: 300, heigth: 500 }}>\n                <ul>\n                    {renderList()}\n                </ul>\n\n            </Scrollbars>\n        </div>\n    )\n\n}\n\nexport default DoubtList","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/OneDoubt/OneDoubt.js",["180"],"import React, { Component } from 'react'\nimport { Link } from 'react-router-dom'\nimport './OneDoubt.css'\n\nconst OneDoubt = (props) => {\n\n    const renderEditButton = () => {\n        if(props.loggedInUser){\n            return(\n                props.selectedDoubt.userId.email !== props.loggedInUser.email \n                    ? null \n                    : <Link to='/editDoubt'>Editar duda</Link>\n            )\n        } else { return null }\n    }\n\n    return(\n        <div className='one-doubt'>\n            <h3>{props.selectedDoubt.title}</h3>\n            <p>{props.selectedDoubt.doubt}</p>\n            <p>Escrita por: {props.selectedDoubt.userId.name} {props.selectedDoubt.userId.lastName}</p>\n\n            { \n            renderEditButton()\n            }\n        </div>\n    )\n\n}\n\nexport default OneDoubt","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/Answers/Answers.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/Message.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/Xat/Xat.js",["181"],"import React, { Component } from 'react'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport './Xat.css'\n\n// --- SERVICES ---\nimport MessageService from '../../../services/message-service'\n\n// --- COMPONENTS ---\nimport Loading from '../../Loading/Loading'\n\nclass Xat extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            newMessage: '',\n            xat: [],\n            toUser: '',\n        }\n        this.MessageService = new MessageService()\n        this.scrollbars = React.createRef()\n    }\n\n    componentDidMount = () => {\n        this.setState({\n            toUser: this.props.toUser,\n            xat: this.props.xat,\n        })\n    }\n\n    componentDidUpdate = () => {\n        this.scrollbars.current.scrollToBottom()\n    }\n\n    handleFormSubmit = (event) => {\n        event.preventDefault()\n        const { newMessage } = this.state\n        this.props.createNewMessage(newMessage)\n        this.setState({\n            newMessage: ''\n        })\n    }\n\n    handleChange = (event) => {\n        const { name, value } = event.target\n        this.setState({ [name]: value })\n    }\n\n\n    renderXat = () => {\n        const xat = [...this.props.xat]\n            return xat.map((message,index) => {\n                return <li key={index}><img src={message.fromUserId.imgPath} /> - {message.message}</li>\n        })\n        \n    }\n\n    render() {\n        return (\n            <div className='xat'>\n                <div className='messages'>\n                    <Scrollbars \n                    ref={this.scrollbars}\n                    style={{ width: 300, height: 250 }}>\n    \n                        <ul>\n                    {this.props.toUser === '' ? <Loading /> : this.renderXat()}\n                        </ul>\n    \n                    </Scrollbars>\n                </div>\n                <form onSubmit={this.handleFormSubmit}>\n                    <input className='input-xat' name='newMessage' value={this.state.newMessage} onChange={ e => this.handleChange(e)} autoComplete='off' placeholder='Escribe tu mensaje' autoFocus />\n                    <button type='submit'>Enviar</button>\n    \n                </form>\n                \n    \n            </div>\n        )\n    }\n}\n\nexport default Xat","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Perfil.js",["182","183","184","185","186"],"import React, { Component } from 'react'\nimport { Link, Switch, Route } from 'react-router-dom'\nimport './Perfil.css'\n\n// --- SERVICES ---\nimport UserService from '../../services/user-service'\nimport MessageService from '../../services/message-service'\nimport DoubtService from '../../services/doubt-services'\n\n// --- COMPONENTS ---\nimport Friends from './Friends/Friends'\nimport Doubts from './Doubts/Doubts'\nimport SearchFriend from './SearchFriend/SearchFriend'\nimport Info from './Info/Info'\nimport Answers from '../PoolDoubts/Answers/Answers'\nimport OneDoubt from '../PoolDoubts/OneDoubt/OneDoubt'\nimport Loading from '../Loading/Loading'\n\n\nclass Perfil extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            friends: [],\n            doubts: [],\n            searchFriends: [],\n            newSearch: [],\n            searchDiv: false,\n            dudas: false,\n            friendsList: true,\n            info: true,\n            selectedDoubt: '',\n            doubtAnswers: ''\n        }\n        this.service = new UserService()\n        this.messageService = new MessageService()\n        this.doubtService = new DoubtService()\n\n    }\n    componentDidMount = (props) => {\n        const userId = this.props.loggedInUser._id\n        this.service\n        .getFriends(userId)\n        .then(result => {\n            this.doubtService.getDoubtsOfUser(userId)\n                .then(response => {\n                    this.setState({\n                        friends: result,\n                        doubts: response\n                    })\n\n                })\n        })\n        this.service\n            .getAllUsers()\n            .then(response => {\n                this.setState({\n                    searchFriends: response,\n                    newSearch: response\n                })\n            })\n    }\n\n    search = (value) => {\n        value !== '' ? this.setState({searchDiv: true}) : this.setState({searchDiv: false})\n        const copyFriends = [...this.state.newSearch]\n        const valueLower = value.toLowerCase()\n        const newArr = copyFriends.filter(item => {\n            const newName = item.name.toLowerCase()\n            return newName.includes(valueLower)\n        })\n        this.setState({\n            searchFriends: newArr\n        })\n        if(value === ''){\n            this.setState({searchFriends: copyFriends})\n        }\n    }\n    renderSearchFriends = () => {\n            return this.state.searchFriends.map((friend, index) => {\n                return (\n                    <Link key={index} to={`/profile/${friend._id}`} onClick={() => this.props.getProfilePublicId(friend._id)}><img src={friend.imgPath} /> {friend.name} {friend.lastName}</Link>\n                )\n            })\n    }\n    showToggle = (e) => {\n        console.log(e.target.id)\n        this.state[e.target.id] ? this.setState({ [e.target.id]:false }) : this.setState({ [e.target.id]:true })\n    }\n    getProfilePublicIdFromFriends = (id) => {\n        return this.props.getProfilePublicId(id)\n    }\n    getDoubt = (id) => {\n        const selectedDoubt = this.state.doubts.filter(doubt => {\n            return doubt._id === id\n        })\n        this.doubtService\n            .getAllAnswersOfDoubt(selectedDoubt[0]._id)\n            .then(answers => {\n                this.setState({\n                    selectedDoubt,\n                    doubtAnswers: answers\n                })\n            })\n    }\n\n\n    render(){\n\n        return (\n            <div className='perfil'>\n                <div className='perfil-head'>\n                    <img src={this.props.loggedInUser.imgPath} />\n                    <h1>¡Hola, {this.props.loggedInUser.name}!</h1>\n                    \n                    \n                </div>\n                <div className='perfil-body'>\n                    \n                    {this.state.info &&\n                        <Info \n                        loggedInUser={this.props.loggedInUser}\n                        />}\n                    <div className='friends-profile'>\n                        \n                        {this.state.friendsList && <Friends\n                        friends={this.state.friends}\n                        getProfilePublicIdFromFriends={this.getProfilePublicIdFromFriends}\n                        />}\n\n                            <SearchFriend\n                        search={this.search}\n                        searchFriends={this.searchFriends}\n                        searchDiv={this.state.searchDiv}\n                        />\n                        <div className='search-friends'>\n\n                            {this.state.searchDiv && this.renderSearchFriends()}\n                        </div>\n\n                    </div>\n                    <div className='perfil-links'>\n                        <Link to='#' id='dudas' onClick={(e) => this.showToggle(e)}>Ver mis dudas</Link>\n                        {/* <Link id='friendsList' onClick={(e) => this.showToggle(e)}>Ver mis amigos</Link>\n                        <Link id='info' onClick={(e) => this.showToggle(e)}>Ver mi información</Link> */}\n\n                    </div>\n\n                    {this.state.dudas && <Doubts\n                    getDoubt={this.getDoubt} \n                    doubts={this.state.doubts} />\n                    }\n                    {(this.state.selectedDoubt && this.state.dudas) && <OneDoubt \n                        selectedDoubt={this.state.selectedDoubt[0]}\n                        loggedInUser={this.props.loggedInUser}\n                    />}\n                    {(this.state.doubtAnswers && this.state.dudas) && <Answers \n                        loggedInUser={this.props.loggedInUser}\n                        doubtAnswers={this.state.doubtAnswers}\n                        getDoubt={this.getDoubt}\n                        selectedDoubt={this.state.selectedDoubt[0]}\n                    />}\n                 \n                        \n                </div>\n\n\n\n            </div>\n        )\n    }\n}\n\nexport default Perfil","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Doubts/Doubts.js",["187"],"import React, { Component } from 'react'\nimport { Scrollbars } from 'react-custom-scrollbars'\n\nconst Doubts = (props) => {\n\n    const handleClick = (e, id) => {\n        e.preventDefault()\n        props.getDoubt(id)\n    }\n\n    const renderDoubts = () => {\n        return props.doubts.map((doubt, index) => {\n            return (\n                <li key={index}>\n                    <button onClick={e => handleClick(e, doubt._id)}>\n                        {doubt.title}\n                    </button>\n                </li>\n            )\n        })\n\n    }\n\n    return (\n        <div>\n            <Scrollbars style={{ width: 300, height: 200}}>\n                <ul>\n                    {renderDoubts()}\n                </ul>\n\n            </Scrollbars>\n        </div>\n    )\n}\n\nexport default Doubts","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/doubt-services.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Loading/Loading.js",["188"],"import React from 'react'\nimport loadingImg from '../../img/devtroopbucle.gif'\n\nconst loading = () => {\n    return (\n        <div>\n            <img src={loadingImg} />\n        </div>\n    )\n}\n\nexport default loading","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/user-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/services/message-service.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Friends/Friends.js",["189","190"],"import React, { Component } from 'react'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport { Link } from 'react-router-dom'\nimport './Friends.css'\n\nconst Friends = (props) => {\n\n    const renderFriends = () => {\n        return props.friends.map((friend, index) => {\n            return (\n                <Link key={index} to={`/profile/${friend._id}`} onClick={() => props.getProfilePublicIdFromFriends(friend._id)}>\n                <div className='card-friend'>\n\n                <img src={friend.imgPath} />\n                <p>\n                  {friend.name} {friend.lastName}  \n                </p>\n                </div>\n                </Link>\n            )\n        })\n    }\n\n    return (\n        <div className='friends'>\n                <h4>Amigos</h4>\n                <p>{props.friends.length} amigos</p>\n            <Scrollbars style={{ width: 300, height: 300 }}>\n                <div className='friends-list'>\n                    {renderFriends()}\n                </div>\n\n            </Scrollbars>\n\n        </div>\n    )\n}\n\nexport default Friends","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Message/XatFriends/XatFriends.js",["191"],"import React, { Component } from 'react'\nimport { Scrollbars } from 'react-custom-scrollbars'\nimport './XatFriends.css'\n\nconst XatFriends = (props) => {\n\n    const renderFriends = () => {\n        return props.friends.map((friend, index) => {\n            return (\n                <li key={index}><button onClick={(e) => handleClick(e, friend._id)}>{friend.name} {friend.lastName}</button></li>\n            )\n        })\n    }\n\n    const handleClick = (e, id) => {\n        e.preventDefault()\n        props.getXatById(id)\n    }\n\n    \n\n    return (\n        <div className='xat-friend'>\n            <Scrollbars style={{ width: 300, height: 300 }}>\n                <ul>\n                    {renderFriends()}\n                </ul>\n            </Scrollbars>\n\n        </div>\n    )\n}\n\nexport default XatFriends","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PoolDoubts/NewDoubt/NewDoubt.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/SearchFriend/SearchFriend.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Footer/Footer.js",[],"/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/PerfilPublico/PerfilPublico.js",["192","193","194"],"import React, { Component } from 'react'\nimport UserService from '../../services/user-service'\nimport './PerfilPublico.css'\n\n\nclass perfilPublico extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            userProfile: '',\n            isFriend: true,\n        }\n        this.userService = new UserService()\n    }\n\n    componentDidMount = () => {\n        this.userService\n            .getUser(this.props.publicProfileId)\n            .then(response => {\n                this.setState({\n                    userProfile: response\n                })\n                this.isFriendToggle(this.props.loggedInUser.friends, response._id)\n            })\n    }\n    renderProfile = () => {\n        return (\n            <div>\n                <img src={this.state.userProfile.imgPath} />\n                <h1>{this.state.userProfile.name} {this.state.userProfile.lastName}</h1>\n                <p><span>Email: </span> {this.state.userProfile.email}</p>\n                <p><span>Ciudad: </span> {this.state.userProfile.city}</p>\n                <p><span>Curso de Ironhack: </span>{this.state.userProfile.ironhackCourse}</p>\n                <p><span>GitHub: </span>{this.state.userProfile.githubUrl && <a href={this.state.userProfile.githubUrl} target='_blank'>Ver perfil</a>}</p>\n                <p><span>Linkedin: </span>{this.state.userProfile.linkedinUrl && <a href={this.state.userProfile.linkedinUrl} target='_blank'>Ver perfil</a>}</p>\n                {\n                    this.state.isFriend\n                    ? this.renderButtonDeleteFriend()\n                    : this.renderButtonFriendship()\n                }\n            </div>\n        )\n    }\n    isFriendToggle = (arr, profileId) => {\n\n        const isFriend = arr.filter(friend => {\n            return friend._id === profileId\n        })\n        if(isFriend.length > 0){\n            this.setState({isFriend: true})\n            return\n        }else {\n            this.setState({isFriend: false})\n            return\n        }\n    }\n    renderButtonFriendship = () => {\n        \n            return(\n                <button onClick={(e) => this.addFriend(e)}>Añadir amistad</button>\n            )\n\n    }\n    renderButtonDeleteFriend = () => {\n            return(\n                <button onClick={(e) => this.deleteFriend(e)}>Eliminar amistad</button>\n            )\n        \n    }\n\n    addFriend = (e) => {\n        e.preventDefault()\n        this.userService\n            .addFriend(this.props.loggedInUser._id, this.state.userProfile._id)\n            .then(response => {\n                this.setState({addedFriend: true})\n            })\n    }\n    deleteFriend = (e) => {\n        e.preventDefault()\n        this.userService\n            .deleteFriend(this.props.loggedInUser._id, this.state.userProfile._id)\n            .then(response => {\n                this.props.updateFriends(response)\n                this.setState({deletedFriend: true})\n            })\n\n    }\n\n\n    render() {\n        return(\n            <div className='perfil-publico'>\n                {this.state.userProfile && this.renderProfile()}\n            </div>\n        )\n\n    }\n}\n\nexport default perfilPublico","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/Perfil/Info/Info.js",["195","196","197"],"import React, { Component } from 'react'\nimport  { Link } from 'react-router-dom'\nimport './Info.css'\n\nconst info = (props) => {\n    return (\n        <div className='profile-info'>\n            <p><span>Nombre: </span>{props.loggedInUser.name}</p>\n            <p><span>Apellido: </span>{props.loggedInUser.lastName}</p>\n            <p><span>Email: </span>{props.loggedInUser.email}</p>\n            <p><span>Ciudad: </span>{props.loggedInUser.city}</p>\n            <p><span>Curso de Ironhack: </span>{props.loggedInUser.ironhackCourse}</p>\n            <a href={props.loggedInUser.githubUrl} target='_blank'>Ver mi perfil de GitHub</a>\n            <a href={props.loggedInUser.linkedinUrl} target='_blank'>Ver mi perfil de Linkedin</a>\n            <Link to='/editProfile'>Modificar información</Link>\n        </div>\n    )\n}\n\nexport default info","/home/mark/Ironhack/bootcamp/semana8/final-project/final-project-front/src/components/EditProfileForm/EditProfileForm.js",["198"],